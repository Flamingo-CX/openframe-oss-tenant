{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "start-vue",
            "type": "npm",
            "script": "dev",
            "path": "services/openframe-ui/",
            "isBackground": true,
            "options": {
                "env": {
                    "API_URL": "http://localhost:8090",
                    "GATEWAY_URL": "http://localhost:8100",
                    "CLIENT_ID": "openframe_web_dashboard_dev",
                    "CLIENT_SECRET": "your_client_secret_here"
                }
            },
            "problemMatcher": {
                "owner": "custom",
                "pattern": {
                    "regexp": "^$"
                },
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": "VITE v",
                    "endsPattern": "ready in"
                }
            }
        }
    ]
} 