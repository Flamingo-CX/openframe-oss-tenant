<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.openframe.agent</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Applications/OpenFrame.app/Contents/MacOS/openframe</string>
        <string>run</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>WorkingDirectory</key>
    <string>/Library/Application Support/OpenFrame</string>
    <key>StandardErrorPath</key>
    <string>/Library/Logs/OpenFrame/error.log</string>
    <key>StandardOutPath</key>
    <string>/Library/Logs/OpenFrame/output.log</string>
    <key>UserName</key>
    <string>root</string>
    <key>GroupName</key>
    <string>wheel</string>
    <key>ProcessType</key>
    <string>Interactive</string>
</dict>
</plist> 