{
  "$schema": "https://raw.githubusercontent.com/Ylianst/MeshCentral/master/meshcentral-config-schema.json",
  "settings": {
    "plugins": {
      "enabled": false
    },
    "mongodb": "mongodb://${MONGODB_USER}:${MONGODB_PASSWORD}@${MONGODB_HOST}:${MONGODB_PORT}/${MONGODB_DB}",
    "certUrl": ["${MESH_NGINX_HOST}:${MESH_EXTERNAL_PORT}", "${MESH_NGINX_NAT_HOST}:${MESH_EXTERNAL_PORT}"],
    "port": ${MESH_PORT},
    "aliasPort": ${MESH_EXTERNAL_PORT},
    "alias": ["${MESH_NGINX_HOST}", "${MESH_NGINX_NAT_HOST}", "${MESH_EXTERNAL_HOST}:${MESH_EXTERNAL_PORT}"],
    "tlsOffload": true,
    "redirPort": 0,
    "AgentPong": 300,
    "SelfUpdate": false,
    "AllowFraming": true,
    "WebRTC": false,
    "IgnoreAgentHashCheck": true,
    "production": false,
    "meshAgentBinaries": {
      "darwin-x64": {
        "compile": true,
        "force": true
      },
      "darwin-arm64": {
        "compile": true,
        "force": true
      }
    }
  },
  "domains": {
    "": {
      "title": "${MESH_DEVICE_GROUP}",
      "title2": "${MESH_DEVICE_GROUP}",
      "minify": true,
      "NewAccounts": false,
      "localSessionRecording": true,
      "allowedOrigin": ["${MESH_NGINX_HOST}", "${MESH_NGINX_NAT_HOST}", "${MESH_EXTERNAL_HOST}"]
    }
  }
}