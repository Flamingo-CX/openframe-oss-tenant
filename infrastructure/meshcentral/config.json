{
  "settings": {
    "datapath": "${MESH_DIR}/data",
    "mongodb": "mongodb://${MONGODB_USER}:${MONGODB_PASSWORD}@${MONGODB_HOST}:${MONGODB_PORT}/${MONGODB_DB}",
    "cert": "${MESH_HOST}",
    "tlsOffload": true,
    "WANonly": false,
    "minify": 1,
    "port": ${MESH_PORT},
    "aliasPort": ${MESH_PORT},
    "redirPort": 0,
    "allowHttpLogin": true,
    "allowHTTP": true,
    "allowLoginToken": true,
    "allowFraming": true,
    "agentPing": 35,
    "allowHighQualityDesktop": true,
    "agentCoreDump": false,
    "compression": ${MESH_COMPRESSION_ENABLED},
    "wsCompression": ${MESH_COMPRESSION_ENABLED},
    "agentWsCompression": ${MESH_COMPRESSION_ENABLED},
    "webRTC": ${MESH_WEBRTC_ENABLED},
    "selfUpdate": false,
    "ignoreFrameOrigin": true,
    "allowAllOrigins": true,
    "allowedRedirectURLs": ["*"],
    "trustedProxy": "*",
    "noIPCheck": true,
    "disableFrameCheck": true,
    "cors": {
      "enabled": true,
      "origins": ["*"]
    },
    "logLevel": "DEBUG",
    "logToFile": true,
    "logPath": "${MESH_DIR}/logs",
    "logMaxSize": 1024,
    "logMaxFiles": 10,
    "certUrl": "http://${MESH_HOST}:${MESH_PORT}/",
    "https": false,
    "cleanNonHttpTraffic": true,
    "lanOnly": true,
    "redirAliasPort": 0,
    "noredirect": true,
    "redirects": false,
    "maxInvalidLogin": {
      "time": 5,
      "count": 5,
      "coolofftime": 30
    },
    "meshAgentBinaries": {
      "darwin-arm64": {
        "compile": true,
        "force": true
      }
    },
    "domains": {
      "": {
        "title": "${MESH_DEVICE_GROUP}",
        "title2": "${MESH_DEVICE_GROUP}",
        "newAccounts": true,
        "certUrl": "http://${MESH_HOST}:${MESH_PORT}/",
        "geoLocation": true,
        "cookieIpCheck": false,
        "autoApprove": true,
        "mstsc": true,
        "_allowNewAccounts": true,
        "_userQuota": 10000,
        "_deviceQuota": 10000
      }
    },
    "smtp": {
      "host": "${SMTP_HOST}",
      "port": ${SMTP_PORT},
      "from": "${SMTP_FROM}",
      "user": "${SMTP_USER}",
      "pass": "${SMTP_PASS}",
      "tls": ${SMTP_TLS}
    }
  }
}