{
  "settings": {
    "datapath": "${TACTICAL_DIR}/data",
    "mongodb": "${ENCODED_URI}",
    "cert": "meshcentral",
    "tlsOffload": true,
    "WANonly": true,
    "minify": 1,
    "port": ${MESH_PORT},
    "aliasPort": ${MESH_PORT},
    "redirPort": 0,
    "allowHttpLogin": true,
    "allowHTTP": true,
    "allowLoginToken": true,
    "allowFraming": true,
    "agentPing": 35,
    "allowHighQualityDesktop": true,
    "agentCoreDump": false,
    "compression": "${MESH_COMPRESSION_ENABLED}",
    "wsCompression": "${MESH_COMPRESSION_ENABLED}",
    "agentWsCompression": "${MESH_COMPRESSION_ENABLED}",
    "webRTC": "${MESH_WEBRTC_ENABLED}",
    "selfUpdate": false,
    "ignoreFrameOrigin": true,
    "trustedProxy": "*",
    "noIPCheck": true,
    "logLevel": "DEBUG",
    "logToFile": true,
    "logPath": "${TACTICAL_DIR}/logs",
    "logMaxSize": 1024,
    "logMaxFiles": 10,
    "certUrl": "http://${MESH_HOST}:${MESH_PORT}/",
    "maxInvalidLogin": {
      "time": 5,
      "count": 5,
      "coolofftime": 30
    },
    "meshAgentBinaries": {
      "darwin-arm64": {
        "compile": true,
        "force": true
      }
    },
    "https": false,
    "cleanNonHttpTraffic": true,
    "postgres": {
      "user": "${POSTGRES_USER}",
      "password": "${POSTGRES_PASSWORD}",
      "port": ${POSTGRES_PORT},
      "host": ${POSTGRES_HOST}
    },
    "domains": {
      "": {
        "title": "Tactical RMM",
        "title2": "Tactical RMM",
        "newAccounts": true,
        "certUrl": "http://${MESH_HOST}:${MESH_PORT}/",
        "geoLocation": true,
        "cookieIpCheck": false,
        "mstsc": true,
        "_allowNewAccounts": true,
        "_userQuota": 10000,
        "_deviceQuota": 10000
      }
    },
    "smtp": {
      "host": "${SMTP_HOST}",
      "port": ${SMTP_PORT},
      "from": "${SMTP_FROM}",
      "user": "${SMTP_USER}",
      "pass": "${SMTP_PASS}",
      "tls": "${SMTP_TLS}"
    }
  }
}